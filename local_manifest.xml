  <project path="vendor/qcom/proprietary" name="proprietary" remote="primo" revision="jellybean" />
  <project path="device/htc/primou" name="primou" remote="primo" revision="aosp-testing" />
  <project path="device/htc/primoc" name="primoc" remote="primo" revision="jellybean" />
